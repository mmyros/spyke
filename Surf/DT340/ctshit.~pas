unit ctshit;

interface

uses
  Windows, Messages, SysUtils, Classes, Graphics, Controls, Forms, Dialogs,
  StdCtrls, OleCtrls, DTAcq32Lib_TLB;

type
  TForm4 = class(TForm)
    CT: TDTAcq32;
    Button1: TButton;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form4: TForm4;

implementation

{$R *.DFM}

procedure TForm4.Button1Click(Sender: TObject);
begin
  CT.Config;
  Showmessage('Configured');
  CT.Start;
  Showmessage('Started');
  Button1.Caption:= Inttostr(CT.CTReadEvents);
end;

end.
