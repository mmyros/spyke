function GetElectrode(var Electrode : TElectrode; Name : ShortString) : boolean;
var i : integer;
begin
   GetElectrode := FALSE;
   For i := 0 to KNOWNELECTRODES-1 do
     if Name = KnownElectrode[i].Name then
     begin
       Move(KnownElectrode[i], Electrode, SizeOf(TElectrode));
       GetElectrode := TRUE;
     end;
end;

initialization
  MakeKnownElectrodes;

end.
