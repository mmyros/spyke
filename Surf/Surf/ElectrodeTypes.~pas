      Outline[5].y := 589;
      Outline[6].x := 32;
      Outline[6].y := 527;
      Outline[7].x := 64;
      Outline[7].y := 465;
      Outline[8].x := 64;
      Outline[8].y := -50;
      Outline[9].x := Outline[0].x;
      Outline[9].y := Outline[0].y;

      NumSites := 16;
      CenterX := 0;
      SiteLoc[4].x := -27;
      SiteLoc[4].y := 155;
      SiteLoc[5].x := -27;
      SiteLoc[5].y := 93;
      SiteLoc[6].x := -27;
      SiteLoc[6].y := 217;
      SiteLoc[7].x := -27;
      SiteLoc[7].y := 341;
      SiteLoc[8].x := -27;
      SiteLoc[8].y := 31;
      SiteLoc[9].x := -27;
      SiteLoc[9].y := 279;
      SiteLoc[10].x := -27;
      SiteLoc[10].y := 403;
      SiteLoc[11].x := -27;
      SiteLoc[11].y := 465;
      SiteLoc[3].x := 27;
      SiteLoc[3].y := 436;
      SiteLoc[2].x := 27;
      SiteLoc[2].y := 372;
      SiteLoc[1].x := 27;
      SiteLoc[1].y := 248;
      SiteLoc[0].x := 27;
      SiteLoc[0].y := 0;
      SiteLoc[15].x := 27;
      SiteLoc[15].y := 310;
      SiteLoc[14].x := 27;
      SiteLoc[14].y := 186;
      SiteLoc[13].x := 27;
      SiteLoc[13].y := 62;
      SiteLoc[12].x := 27;
      SiteLoc[12].y := 124;
    end;


    // 16CHAN5 came from Steve Bierre
    with KnownElectrode[4] do
    begin
      Name := '16Chan5';
      Description := '16 Chan Silicon, 4 Shank';
      SiteSize.x := 10;
      SiteSize.y := 10;
      RoundSite := TRUE;
